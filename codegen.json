{
  "overwrite": true,
  "schema": "${GRAPHQL_ENDPOINT}",
  "documents": "src/lib/graphql/**/*.graphql",
  "generates": {
    "src/lib/generated/graphql.ts": {
      "plugins": ["typescript", "typescript-operations", "typescript-document-nodes"],
      "./graphql.schema.json": {
        "plugins": "introspection"
      }
    }
  }
}
